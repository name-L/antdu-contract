    "name": "ant-up-tree-main-app",
    "title": "Ant Up Tree Project (main)",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start-mock": "node app.mock.js",
        "start-dev": "babel-node --plugins --trace-deprecation transform-es2015-modules-commonjs app.dev.js",
        "start-dev- debug-win": "babel-node %NODE_DEBUG_OPTION% --trace-deprecation --plugins transform-es2015-modules-commonjs app.dev.js",
        "start-prod": "cd dist && node app.js",
        "analysis-json": "cross-env TARGET=www MODE=development webpack --progress --config build/webpack.config.prod.js --profile --json > webpack-stats.json",
        "analysis-bundle": "cross-env TARGET=www ANALYSIS=true MODE=development webpack --progress --config build/webpack.config.prod.js",
        "analysis-build": "cross-env TARGET=www MODE=development webpack --progress --config build/webpack.config.prod.js && npm run server-es6",
        "build-dev": "cross-env TARGET=dev webpack --display-modules --display-chunks --progress --config build/webpack.config.prod.js && npm run server-es6 && cd dist && npm prune --production",
        "build-exp": "cross-env TARGET=exp webpack --display-modules --display-chunks --progress --config build/webpack.config.prod.js && npm run server-es6 && cd dist && npm prune --production",
        "build-www": "cross-env TARGET=www webpack --display-modules --display-chunks --progress --config build/webpack.config.prod.js && npm run server-es6 && cd dist && npm prune --production",
        "server-es6": "cross-env babel app.prod.js -q -o dist/app.js --plugins transform-es2015-modules-commonjs && babel ./server -d dist/server --plugins transform-es2015-modules-commonjs",
        "dist-update": "cd dist && npm install",
        "lint": "eslint --ext .js,.vue -f eslint-formatter-pretty build client config server app.*.js",
        "lint-fix": "eslint --ext .js,.vue -f eslint-formatter-pretty --fix build client config server app.*.js"
    },
